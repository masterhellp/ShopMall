<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="personal_title_text" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/personal_title_text_size</item>
    </style>

    <style name="personal_recommend_container" >
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">10.5dp</item>
        <item name="android:layout_marginRight">10.5dp</item>
        <item name="android:padding">0dp</item>
    </style>

    <style name="personal_recommend_image" >
        <item name="android:layout_width">@dimen/cart_recommend_image_height</item>
        <item name="android:layout_height">@dimen/cart_recommend_image_height</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">fitCenter</item>
    </style>

    <style name="personal_recommend_title" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">2</item>
        <item name="android:minLines">2</item>
        <item name="android:textColor">@color/text_color_686868</item>
        <item name="android:textSize">11sp</item>
        <item name="android:padding">0dp</item>
        <item name="android:lineSpacingExtra">2dp</item>
    </style>

    <style name="personal_recommend_price" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/text_color_252525</item>
        <item name="android:textSize">12sp</item>
    </style>


    <style name="my_street_item_content1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/my_street_recommend_item_bg</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>

    <style name="my_street_item_content">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="my_street_item_lefttop_container_parent">
        <item name="android:layout_width">44dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="my_street_item_lefttop_container">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/my_street_item_tag_leftbg</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>

    <style name="my_street_item_left_line">
        <item name="android:layout_width">2.5dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@color/gray</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="my_street_item_lefttop_img">
        <item name="android:layout_width">28dp</item>
        <item name="android:layout_height">28dp</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="my_street_item_time">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="my_street_item_name">
        <item name="android:textColor">#252525</item>
        <item name="android:padding">2dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">2</item>
        <item name="android:minLines">2</item>
        <item name="android:layout_marginTop">1dp</item>
    </style>

    <style name="my_street_item_tag">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">#ea3a42</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">11sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="my_street_item_tag_sales">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">#ea3a42</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">11sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:text">"降"</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="my_street_item_tag_describe">
        <item name="android:textColor">#686868</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">11sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="my_street_item_price">
        <item name="android:textColor">#f15353</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:singleLine">true</item>
        <item name="android:text">暂无报价</item>
    </style>

    <style name="my_street_item_price_sales">
        <item name="android:textColor">#686868</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">11sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:visibility">gone</item>
        <item name="android:singleLine">true</item>
    </style>

</resources>
